                                            android:text="Harga Emas Dunia Naik Tajam Didorong Ekspektasi Pelambatan Kenaikan Suku Bunga The Fed"
                                            android:textColor="#2e74b2"
                                            android:textSize="12sp"
                                            android:textStyle="bold"
